<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.media.MediaView?>

<AnchorPane prefHeight="700.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <BorderPane layoutY="3.0" prefHeight="550.0" prefWidth="500.0">
         <center>
            <MediaView fx:id="mv" fitHeight="600.0" fitWidth="500.0" BorderPane.alignment="CENTER" />
         </center>
      </BorderPane>
      <HBox layoutY="660.0" prefHeight="40.0" prefWidth="500.0">
         <children>
            <Button mnemonicParsing="false" onAction="#play" text="Play" />
            <Button mnemonicParsing="false" onAction="#pause" text="Pause" />
            <Button mnemonicParsing="false" onAction="#fast" text="Fast" />
            <Button mnemonicParsing="false" onAction="#slow" text="Slow" />
            <Button mnemonicParsing="false" onAction="#reload" text="Reload" />
            <Button mnemonicParsing="false" onAction="#start" text="Start" />
            <Button mnemonicParsing="false" onAction="#last" text="Last" />
            <Slider fx:id="volumeSlider" />
         </children>
      </HBox>
   </children>
</AnchorPane>
